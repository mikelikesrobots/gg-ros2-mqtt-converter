<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>ros2_mqtt_converter</name>
  <version>1.0.0</version>
  <description>ROS2 node to subscribe to ROS2 topics and publish JSON conversions over pub/sub</description>
  <maintainer email="mikelikesrobots@outlook.com">Mike Likes Robots</maintainer>
  <license>MIT-0</license>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
